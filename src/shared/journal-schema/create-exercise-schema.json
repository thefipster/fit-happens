{
  "$schema": "https://json-schema.org/draft-07/schema",
  "title": "Create Exercise Message",
  "type": "object",
  "properties": {
    "type": {
      "type": "string"
    },
    "exerciseId": {
      "type": "string"
    },
    "exerciseType": {
      "type": "string",
      "enum": ["reps", "time"]
    },
    "name": {
      "type": "string"
    },
    "journalId": {
      "type": "string"
    },
    "timestamp": {
      "type": "number"
    }
  },
  "required": [
    "type",
    "exerciseId",
    "exerciseType",
    "name",
    "journalId",
    "timestamp"
  ]
}